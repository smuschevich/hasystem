//
// Do any local configuration here
//

// Consider adding the 1918 zones here, if they are not used in your
// organization
//include "/etc/bind/zones.rfc1918";

zone "hasystem.com" {
    type master;
    file "/etc/bind/db.hasystem.com";
};

zone "0.7.10.in-addr.arpa" {
    type master;
    file "/etc/bind/db.10";
};

zone "service.local" {
    type master;
    file "/etc/bind/db.service.local";
};
